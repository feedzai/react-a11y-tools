{
	"name": "@feedzai/react-a11y-tools",
	"author": "Feedzai",
	"version": "1.0.2",
	"license": "MIT",
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"module": "dist/index.esm.js",
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"start": "start-storybook -p 6006",
		"build": "rollup -c",
		"format:fix": "npm run prettier:base -- --write 'src/**/*.{ts,tsx,js,jsx}'",
		"lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
		"typecheck": "tsc --project ./tsconfig.json",
		"prettier:base": "prettier --parser typescript",
		"prepare": "npm run typecheck",
		"prebuild": "npm run typecheck",
		"postbuild": "npx size-limit",
		"storybook:ci": "npm run start -- --ci",
		"build:homepage": "cp -a ./documentation/. public/",
		"build:storybook": "build-storybook -c .storybook -o public/docs",
		"build:documentation": "npm run build:storybook && npm run build:homepage",
		"reinstall": "shx rm -rf node_modules && npm install",
		"pretest": "shx rm -rf .nyc_output jest-coverage cypress-coverage reports coverage || true",
		"report:combined": "node ./scripts/merge-coverage.js",
		"test:jest": "jest --env=jsdom --coverage --passWithNoTests",
		"test:jest-ci": "jest --ci --coverage --maxWorkers=2",
		"test:watch": "jest --watch",
		"test:cypress-components": "cypress run-ct",
		"test:components": "npm run test:jest && npm run test:cypress-components",
		"test:cypress": "cypress open",
		"test:e2e": "start-server-and-test storybook:ci 6006 'cypress run'",
		"test": "npm run test:components && npm run test:e2e && npm run report:combined"
	},
	"peerDependencies": {
		"react": ">=16.14.0",
		"react-dom": ">=16.14.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/feedzai/react-a11y-tools.git"
	},
	"devDependencies": {
		"@babel/core": "7.15.0",
		"@babel/plugin-proposal-optional-chaining": "7.14.5",
		"@babel/preset-env": "7.15.0",
		"@babel/preset-react": "7.14.5",
		"@babel/preset-typescript": "7.15.0",
		"@cypress/code-coverage": "3.9.10",
		"@cypress/react": "5.9.4",
		"@cypress/snapshot": "2.1.7",
		"@cypress/webpack-dev-server": "1.4.0",
		"@emotion/react": "11.4.1",
		"@emotion/styled": "11.3.0",
		"@rollup/plugin-commonjs": "20.0.0",
		"@size-limit/preset-small-lib": "5.0.3",
		"@storybook/addon-actions": "6.3.7",
		"@storybook/addon-essentials": "6.3.7",
		"@storybook/addon-info": "5.3.21",
		"@storybook/addon-links": "6.3.7",
		"@storybook/addons": "6.3.7",
		"@storybook/builder-webpack5": "6.3.7",
		"@storybook/manager-webpack5": "6.3.7",
		"@storybook/react": "6.3.7",
		"@storybook/theming": "6.3.7",
		"@testing-library/cypress": "8.0.0",
		"@testing-library/jest-dom": "5.14.1",
		"@testing-library/react": "12.0.0",
		"@testing-library/react-hooks": "7.0.1",
		"@testing-library/user-event": "13.2.1",
		"@types/classnames": "2.3.0",
		"@types/jest": "26.0.23",
		"@types/jsdom": "16.2.13",
		"@types/node": "16.6.2",
		"@types/react": "17.0.19",
		"@types/react-dom": "17.0.9",
		"@typescript-eslint/eslint-plugin": "4.29.2",
		"@typescript-eslint/parser": "4.29.2",
		"autoprefixer": "10.3.1",
		"babel-jest": "27.0.2",
		"babel-loader": "8.2.2",
		"babel-plugin-istanbul": "6.0.0",
		"babel-plugin-styled-components": "1.13.2",
		"chalk": "4.1.2",
		"check-code-coverage": "1.10.0",
		"css-loader": "6.2.0",
		"cypress": "8.3.0",
		"cypress-multi-reporters": "1.5.0",
		"cypress-plugin-tab": "1.0.5",
		"cypress-real-events": "1.5.1",
		"esbuild": "0.12.21",
		"eslint": "7.32.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-import-resolver-typescript": "2.4.0",
		"eslint-plugin-cypress": "2.11.3",
		"eslint-plugin-import": "2.24.1",
		"eslint-plugin-jest": "24.4.0",
		"eslint-plugin-jsx-a11y": "6.4.1",
		"eslint-plugin-prettier": "3.4.0",
		"eslint-plugin-react": "7.24.0",
		"eslint-plugin-react-hooks": "4.2.0",
		"fs": "0.0.1-security",
		"history": "5.0.1",
		"html-webpack-plugin": "5",
		"husky": "7.0.1",
		"identity-obj-proxy": "3.0.0",
		"istanbul-lib-coverage": "3.0.0",
		"jest": "26.4.2",
		"jsdom": "16.6.0",
		"lint-staged": "11.1.2",
		"mochawesome": "6.2.2",
		"mochawesome-merge": "4.2.0",
		"mochawesome-report-generator": "5.2.0",
		"node-sass": "5.0.0",
		"nyc": "15.1.0",
		"path-browserify": "^1.0.1",
		"postcss": "8.3.6",
		"prettier": "2.3.2",
		"raf": "3.4.1",
		"react": ">=17.0.2",
		"react-dom": ">=17.0.2",
		"react-is": "17.0.2",
		"react-test-renderer": "17.0.2",
		"rollup": "2.56.2",
		"rollup-plugin-analyzer": "4.0.0",
		"rollup-plugin-dts": "3.0.2",
		"rollup-plugin-esbuild": "4.5.0",
		"rollup-plugin-peer-deps-external": "2.2.4",
		"rollup-plugin-postcss": "4.0.1",
		"sass": "1.38.0",
		"sass-loader": "12.1.0",
		"shx": "0.3.3",
		"size-limit": "5.0.3",
		"start-server-and-test": "1.13.1",
		"ts-jest": "26.5.5",
		"ts-loader": "9.2.5",
		"tslib": "2.3.1",
		"typescript": "4.3.5",
		"webpack": "5.51.1",
		"webpack-dev-server": "3.11.2"
	},
	"browser": {
		"fs": false
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,html}": [
			"npm run format:fix",
			"npm run lint"
		]
	},
	"size-limit": [
		{
			"path": "dist/index.js",
			"limit": "8 kB",
			"ignore": [
				"fs"
			]
		},
		{
			"path": "dist/index.esm.js",
			"limit": "8 kB",
			"ignore": [
				"fs"
			]
		}
	],
	"nyc": {
		"report-dir": "cypress-coverage",
		"reporter": [
			"json"
		],
		"include": [
			"src/**/*"
		],
		"exclude": [
			".storybook/**",
			"test/**",
			"stories/**"
		]
	},
	"np": {
		"testScript": "test:jest",
		"contents": "dist"
	}
}
