{
	"name": "@feedzai/react-a11y-tools",
	"author": "Feedzai",
	"version": "1.4.1",
	"license": "MIT",
	"main": "./dist/index.cjs.cjs",
	"types": "./dist/index.d.ts",
	"module": "./dist/index.es.mjs",
	"exports": {
		".": {
			"import": "./dist/index.es.cjs",
			"require": "./dist/index.cjs.mjs",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"engine": {
		"node": ">= 18",
		"npm": ">= 10"
	},
	"scripts": {
		"build": "vite build",
		"format:fix": "npm run prettier:base -- --write 'src/**/*.{ts,tsx,js,jsx}'",
		"lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
		"typecheck": "tsc --project ./tsconfig.json",
		"prettier:base": "prettier --parser typescript",
		"postbuild": "npx size-limit",
		"postinstall": "cd ./documentation && npm install",
		"docs:start": "cd ./documentation && npm run start",
		"docs:start-ci": "cd ./documentation && npm run start",
		"docs:build": "cd ./documentation && npm run build",
		"docs:deploy": "cd ./documentation && GIT_USER=feedzai && npm run deploy",
		"reinstall": "rm -rf node_modules && npm install",
		"coverage:clean": "rm -rf .nyc_output coverage-reports coverage || true",
		"coverage:merge": "node ./scripts/merge-coverage.js",
		"pretest": "npm run coverage:clean",
		"test:jest": "jest --env=jsdom --coverage --passWithNoTests",
		"test:jest-watch": "jest --env=jsdom --watch --coverage --passWithNoTests",
		"test:jest-ci": "jest --env=jsdom --ci --coverage --maxWorkers=2",
		"test:component-open": "NODE_ENV=cypress cypress open --component --browser chrome",
		"test:component-run": "NODE_ENV=cypress cypress run --component --browser chrome",
		"test:integration-open": "NODE_ENV=cypress cypress open --e2e",
		"test:integration-run": "NODE_ENV=cypress cypress run --e2e --browser chrome",
		"test:integration": "start-server-and-test docs:start-ci http://localhost:3000/react-a11y-tools/ 'npm run test:integration-run'",
		"test": "npm run test:jest && npm run test:component-run && npm run test:integration && npm run coverage:merge",
		"test:ci": "npm run test:jest-ci && npm run test:component-run && npm run test:integration"
	},
	"peerDependencies": {
		"react": ">=16.14.0",
		"react-dom": ">=16.14.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/feedzai/react-a11y-tools.git"
	},
	"devDependencies": {
		"@babel/core": "7.23.9",
		"@babel/helper-environment-visitor": "7.18.9",
		"@babel/helper-string-parser": "^ 7.19.4",
		"@babel/plugin-proposal-optional-chaining": "7.18.9",
		"@babel/preset-env": "7.19.4",
		"@babel/preset-react": "7.18.6",
		"@babel/preset-typescript": "7.18.6",
		"@cypress/code-coverage": "3.12.24",
		"@size-limit/preset-small-lib": "11.0.2",
		"@testing-library/cypress": "10.0.1",
		"@testing-library/jest-dom": "6.4.2",
		"@testing-library/react": "14.2.1",
		"@testing-library/user-event": "14.5.2",
		"@types/classnames": "2.3.0",
		"@types/jest": "29.5.12",
		"@types/jsdom": "16.2.13",
		"@types/node": "16.11.3",
		"@types/react": "18.2.58",
		"@types/react-dom": "18.2.19",
		"@typescript-eslint/eslint-plugin": "5.1.0",
		"@typescript-eslint/parser": "5.1.0",
		"@vitejs/plugin-react": "2.1.0",
		"autoprefixer": "10.4.7",
		"axe-core": "4.8.4",
		"babel-jest": "29.7.0",
		"babel-loader": "9.1.3",
		"babel-plugin-istanbul": "6.1.1",
		"babel-plugin-styled-components": "1.13.3",
		"chalk": "4.1.2",
		"cypress": "13.6.4",
		"cypress-axe": "1.5.0",
		"cypress-multi-reporters": "1.6.4",
		"cypress-real-events": "1.12.0",
		"cypress-recurse": "1.35.2",
		"esbuild": "0.14.47",
		"eslint": "8.56.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-import-resolver-typescript": "3.6.1",
		"eslint-plugin-cypress": "2.15.1",
		"eslint-plugin-import": "2.29.1",
		"eslint-plugin-jest": "27.9.0",
		"eslint-plugin-jsx-a11y": "6.8.0",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-react": "7.33.2",
		"eslint-plugin-react-hooks": "4.6.0",
		"fs": "0.0.1-security",
		"history": "5.3.0",
		"husky": "7.0.4",
		"identity-obj-proxy": "3.0.0",
		"istanbul-lib-coverage": "3.2.2",
		"jest": "29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jsdom": "16.6.0",
		"lint-staged": "11.2.3",
		"nyc": "15.1.0",
		"postcss": "^8.4.35",
		"prettier": "2.7.1",
		"raf": "3.4.1",
		"react-dom": "^18.0.0",
		"react": "^18.0.0",
		"react-is": "18.2.0",
		"react-router": "^6.22.1",
		"react-router-dom": "^6.22.1",
		"sass": "1.53.0",
		"size-limit": "11.0.2",
		"start-server-and-test": "2.0.3",
		"ts-jest": "29.1.2",
		"typescript": "4.7.4",
		"vite": "^3.2.8",
		"vite-plugin-dts": "^1.6.6",
		"vite-plugin-istanbul": "3.0.1"
	},
	"browser": {
		"fs": false
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,html}": [
			"npm run format:fix",
			"npm run lint"
		]
	},
	"size-limit": [
		{
			"path": "dist/index.cjs.cjs",
			"limit": "10 kB",
			"ignore": [
				"fs"
			]
		},
		{
			"path": "dist/index.es.mjs",
			"limit": "10 kB",
			"ignore": [
				"fs"
			]
		}
	],
	"nyc": {
		"all": true,
		"report-dir": "coverage-reports/cypress",
		"reporter": [
			"lcov",
			"json"
		],
		"check-coverage": true,
		"watermarks": {
			"statements": 80,
			"branches": 70,
			"functions": 80,
			"lines": 80
		},
		"include": [
			"src/**/*"
		]
	}
}
